    <object id="66_24309637" type="text" x="75529.40119706927" y="-12004.993088683386" r="-1.3777449072832227e-15" s="11.101432150642815" class="body">
      <height>551.953125</height>
      <width>1299.0740200700436</width>
      <p listType="bullet" indent="1">
        <text color="0"><![CDATA[Initially set min to a large value which is equivalent infinity]]></text>
      </p>
      <p indent="4">
        <text color="0"><![CDATA[min=999;]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="0"><![CDATA[Store the Source and Destination routers ]]></text>
      </p>
      <p indent="4">
        <text color="0"><![CDATA[src=x;     des=y;]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="0"><![CDATA[Check whether min is greater than Distance to a particular node and the node is not visited]]></text>
      </p>
      <p indent="3">
        <text color="0"><![CDATA[      if(min>distance[k][j] && visited[k][j]!=1) ]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="0"><![CDATA[If this is satisfied, min value is updated to the lesser cost distance]]></text>
      </p>
      <p indent="3">
        <text color="0"><![CDATA[      min=distance[k][j]; ]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="0"><![CDATA[Minimum distance node is assigned to nextnode.]]></text>
      </p>
      <p indent="3">
        <text color="0"><![CDATA[      nextnode=j;]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="0"><![CDATA[ Visited from source to next node is changed to 1.]]></text>
      </p>
      <p indent="3">
        <text color="0"><![CDATA[      visited[k][nextnode]=1;]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="0"><![CDATA[If the given node is not visited then check whether the min value and cost for nextnode is less than distance ]]></text>
      </p>
      <p indent="2">
        <text color="0"><![CDATA[           if(visited[k][c]!=1)        ]]></text>
      </p>
      <p indent="3">
        <text color="0"><![CDATA[      if(min+matrix[nextnode][c]<distance[k][c]) ]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="0"><![CDATA[If the condition satisfies the distance is updated with min + cost of the nextnode and pred stores the path traversed(Shortest path).]]></text>
      </p>
      <p indent="3">
        <text color="0"><![CDATA[     distance[k][c]=min+matrix[nextnode][c];  ]]></text>
      </p>
      <p indent="1">
        <text color="0"><![CDATA[                pred[k][c]=nextnode; ]]></text>
      </p>
      <p>
        <text><![CDATA[]]></text>
      </p>
      <layout>
        <layout-element role="body" parent-id="8_24309637"/>
      </layout>
    </object>
